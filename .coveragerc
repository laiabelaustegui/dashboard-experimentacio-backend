[run]
source = .
omit = 
    */migrations/*
    */tests.py
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    */__init__.py
    manage.py
    */asgi.py
    */wsgi.py
    */settings.py
    */apps.py
    */admin.py
    */celery.py
    */tasks.py
    run_tests.py
    run_tests.ps1

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
